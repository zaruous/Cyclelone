{
	"list": [
		{
			"version": 1,
			"path": "/uml",
			"controller" : "com.kyj.api.cyclelone.impl.uml.UMLController",
			"description": "URML 핸들링",
			"methods": [
                {
                    "name": "getUML",
                    "method": "POST",
                    "description": "UML 생성",
                    "parameters": [
                        {
                            "name": "content",
                            "type": "String",
                            "description": "UML 생성 컨텐츠"
                        }
                    ],
                    "responses": [
                        {
                            "status": 200,
                            "description": "성공적으로 UML 정보를 반환합니다."
                        },
                        {
                            "status": 404,
                            "description": "UML 정보를 찾을 수 없습니다."
                        }
                    ]
                }
            ]
			
		},
		{
			"version": 1,
			"path": "/suno",
			"controller" : "com.kyj.api.cyclelone.impl.suno.SunoController",
			"description": "음악 처리 핸들링",
			"methods": [
                {
                    "name": "generate",
                    "method": "POST",
                    "description": "생성",
                    "parameters": [
                        {
                            "name": "content",
                            "type": "String",
                            "description": "UML 생성 컨텐츠"
                        }
                    ],
                    "responses": [
                        {
                            "status": 200,
                            "description": "success."
                        },
                        {
                            "status": 404,
                            "description": "failed."
                        }
                    ]
                }
            ]
		},
		{
			"version": 1,
			"path": "/slack/message",
			"controller" : "com.kyj.api.cyclelone.impl.slack.SlackController",
			"description": "음악 처리 핸들링",
			"methods": [
                {
                    "name": "sendMessage",
                    "method": "POST",
                    "description": "메세지전송",
                    "parameters": [{
                         "name": "message",
                         "type": "String",
                         "description": "메세지"
                    }],
                    "responses": [
                        {
                            "status": 200,
                            "description": "success."
                        },
                        {
                            "status": 404,
                            "description": "failed."
                        }
                    ]
                }
            ]
		}
	]
}